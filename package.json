{
  "name": "iesna",
  "version": "0.0.0",
  "author": "Richard Viney",
  "license": "MIT",
  "repository": "https://github.com/richard-viney/iesna.git",
  "type": "module",
  "files": [
    "dist",
    "src",
    "LICENSE.md",
    "README.md"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "format": "prettier --write \"**/*.js\" \"**/*.ts\"",
    "format:check": "prettier --check \"**/*.js\" \"**/*.ts\"",
    "lint": "eslint src",
    "prepare": "tsc && tsc-esm-fix --target=dist",
    "test": "vitest"
  },
  "devDependencies": {
    "canvas": "^3.2.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
